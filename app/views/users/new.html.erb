<h1>Create Account</h1>

<%= form_for (@user) do |f| %>
    <%= f.label :username %>: 
    <%= f.text_field :username %>
    <br>
    <%= f.label :email %>: 
    <%= f.email_field :email %> 
    <br>
    <%= f.label :age %>: 
    <%= f.text_field :age %>
    <br>
    <%= f.label :goal %>: 
    <%= f.text_field :goal %> 
    <br>
    <%= f.label :password %>: 
    <%= f.password_field :password %> 
<br>
<br>
    <%= f.submit "Create Fitness Journal" %> 

    <% end %> 
